{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "input_file",
						"type": "DatasetReference"
					},
					"name": "source"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filtercolumns"
				}
			],
			"scriptLines": [
				"source(output(",
				"          order_id as integer,",
				"          order_item_id as string,",
				"          product_id as string,",
				"          seller_id as string,",
				"          shipping_limit_date as string,",
				"          price as float,",
				"          freight_value as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source",
				"source filter(price >='100') ~> filtercolumns"
			]
		}
	}
}